    "\n",
    "    # Generate the summary\n",
    "    summary_ids = model.generate(**inputs, max_length=128)\n",
    "    predicted_summary = tokenizer.decode(summary_ids[0], skip_special_tokens=True)\n",
    "\n",
    "    return predicted_summary\n"
